<%= tinymce %>
<div class="nested-fields">
        
        <div class="input_field" >
        <%= f.text_field :title, input_html: { class: 'materialize_textarea' }, as: :text ,style: 'width: 500px; height: 80px' %>
        
         </div>
         
         <div class="input_field">
             <%= f.text_area :content, :class => "tinymce", :rows => 40, :cols => 120 %>
         </div>
         
      <div class="links">
        <% link_to_remove_association 'x', f , class: 'btn-floating btn-large waves-effect waves-light red' %>
       </div>
  </div>